{
  "name": "lyme",
  "version": "1.0.0",
  "description": "A chat App which name sounds like LINE, built by Nuxt and PWA",
  "author": "John",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:sw": "webpack --config sw.webpack.config.js --watch",
