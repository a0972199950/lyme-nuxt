    "@types/config": "^0.0.36",
    "@types/socket.io-client": "^1.4.32",
    "config": "^3.3.1",
    "idb": "^5.0.2",
    "npm": "^6.14.4",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.5.1",
    "socket.io-client": "^2.3.0",
    "sweetalert2": "^9.10.12",
    "ts-node": "^8.8.2",
    "web-push": "^3.4.3",
    "wired-elements": "^2.0.5"
